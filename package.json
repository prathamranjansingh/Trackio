{
  "name": "Trackio",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "package:vscode": "turbo run build --filter=codetracker && npm run package --workspace=codetracker"
  },
  "devDependencies": {
    "@types/node": "^24.5.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.4.2",
  "workspaces": [
    "apps/*",
    "packages/*",
    "cli/*"
  ],
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "motion": "^12.23.24",
    "react-day-picker": "^8.10.1",
    "react-hook-form": "^7.65.0",
    "recharts": "^3.2.1",
    "tsup": "8.5.0",
    "vercel": "^48.2.7"
  },
  "resolutions": {
    "next": "15.5.2",
    "next-auth": "4.24.11"
  },
  "overrides": {
    "next": "15.5.2",
    "next-auth": "4.24.11",
    "react-day-picker": {
      "react": "19.1.1"
    }
  }
}
